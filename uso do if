#include <stdio.h>
int main()


{
    int num;
    
    printf("insira um numero :");
    scanf("%d",&num);
    
    
    
    if (num == 10){
       
        printf("o numero inserido e igual a 10.\n");
    }
        if(num>10){
        printf("o numero inserido e maior que 10.\n");
        }
        if(num<10){
        printf("o numero inserido e menor que 10.\n");
    }
        

    
    
    
    
    
    
    

    return 0;
}
